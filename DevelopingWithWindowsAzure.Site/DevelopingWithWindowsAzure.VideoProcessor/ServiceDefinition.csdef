<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="DevelopingWithWindowsAzure.VideoProcessor" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2012-05.1.7">
  <WorkerRole name="VideoProcessorRole" vmsize="Small">
    <Imports>
      <Import moduleName="Diagnostics" />
    </Imports>
    <ConfigurationSettings>
			<Setting name="DatabaseConnectionString"/>
			<Setting name="StorageConnectionString"/>
			<Setting name="ServiceBusQueueName" />
			<Setting name="Microsoft.ServiceBus.ConnectionString" />
    </ConfigurationSettings>
  </WorkerRole>
</ServiceDefinition>